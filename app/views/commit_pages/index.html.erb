<p id="notice"><%= notice %></p>

<h1>Commit Pages</h1>

<table>
  <thead>
    <tr>
      <th>Top commit</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @commit_pages.each do |commit_page| %>
      <tr>
        <td><%= commit_page.top_commit %></td>
        <td><%= link_to 'Show', commit_page %></td>
        <td><%= link_to 'Edit', edit_commit_page_path(commit_page) %></td>
        <td><%= link_to 'Destroy', commit_page, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Commit Page', new_commit_page_path %>
